/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": ["src/app/*"],
      "@classes/*": ["src/app/classes/*"],
      "@core/*": ["src/app/core/*"],
      "@data/*": ["src/app/data/*"],
      "@interfaces/*": ["src/app/interfaces/*"],
      "@pages/*": ["src/app/pages/*"],
      "@auth/*": ["src/app/pages/auth/*"],
      "@services/*": ["src/app/services/*"],
      "@shared/*": ["src/app/shared/*"],
      "@assets/*": ["src/assets/*"],
      "@environments/*": ["src/environments/*"],
      "@pipes/*": ["src/app/pipes/*"],
      "@validators/*": ["src/app/validators/*"],
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
